{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "../out",
		"allowJs": true,
		"noImplicitReturns": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"rootDirs": [
			".",
			"../client/out"
		]
	},
	"exclude": [
		"node_modules"
	]
}